{
  "name": "sanity-plugin-vercel-deploy",
  "version": "3.3.4",
  "description": "Trigger Vercel Deploy Hooks from your Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "vercel",
    "deploy",
    "webhook"
  ],
  "homepage": "https://github.com/ndimatteo/sanity-plugin-vercel-deploy#readme",
  "bugs": {
    "url": "https://github.com/ndimatteo/sanity-plugin-vercel-deploy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ndimatteo/sanity-plugin-vercel-deploy.git"
  },
  "license": "MIT",
  "author": "Nick DiMatteo <hello@nickdimatteo.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint '**/*.+(ts|js|tsx|jsx)'",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^1.0.8",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.14",
    "axios": "^0.24.0",
    "nanoid": "^3.1.20",
    "prop-types": "^15.7.2",
    "spacetime": "^6.16.0",
    "swr": "^1.1.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.2.3",
    "@sanity/plugin-kit": "^3.1.4",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.1.2",
    "sanity": "^3.2.5",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
